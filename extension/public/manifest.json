{
  "name": "Basic Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Basic Chrome Extension for boiletplate",
  "icons": {
      "512": "square-logo.png"
  },
  "browser_action": {
      "default_icon": "square-logo.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": "object-src 'self'; script-src 'self' https://apis.google.com/js/api.js 'unsafe-eval';",
  "permissions": [
      "identity",
      "identity.email",
      "https://accounts.google.com/o/oauth2/token"
    ]
}
